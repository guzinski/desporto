<!DOCTYPE html>
<html lang="br">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>{% block title %}{% endblock %}</title>

        <link href="{{ asset('bundles/desporto/css/style.default.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/desporto/css/morris.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/desporto/css/select2.css') }}" rel="stylesheet">
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

        
        <script src="{{ asset('bundles/desporto/js/jquery-1.11.1.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/jquery-migrate-1.2.1.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/modernizr.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/pace.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/retina.min.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/jquery.cookies.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/custom.js') }}"></script>
        <script src="{{ asset('bundles/desporto/js/bootbox.min.js') }}"></script>        

        {% block head %}{% endblock %}
    </head>
    <body>
        <div class="locked" style="display: none">
            <div class="lockedpanel">
                <div class="loginuser">
                    <img src="{{ asset('bundles/desporto/images/loaders/loader7.gif') }}" class="" alt="" />
                </div>
            </div><!-- lockedpanel -->
        </div><!-- locked -->
       
        <header>
            {% include 'DesportoBundle::header.html.twig'  %}
        </header>

        <section>
            <div class="mainwrapper">
                {% include 'DesportoBundle::menu.html.twig'  %}
                <div class="mainpanel">
                    {% block mainpanel %}{% endblock %}
                </div>
            </div>
        </section>
        
    </body>
</html>